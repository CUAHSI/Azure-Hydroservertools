@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request. Please try to refresh the page or log out and back in. If the problem persists please each out to mailto:help@cuahsi.org?subject=Log%20in%20error%20Hydroserver. Thank you</h2>

<h2>
    @if (@ViewBag.Message != null)
    {
        @ViewBag.Message

    }
</h2>