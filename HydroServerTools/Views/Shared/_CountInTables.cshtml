<div class="panel panel-default  col-lg-4 col-lg-offset-0">
    <div class="panel-heading">
        <h3 class="panel-title">Status</h3>
    </div>
    <div class="panel-body">
        <p><strong>Last Modified:</strong> @ViewBag.UpdateDateTime<a href="#" data-toggle="tooltip" style="padding-left:20px" title="Last time you added, deleted or modified your data."><img class="infobutton" src="~/Images/infobutton_60.jpg" /></a></p>
        <p><strong>Last Published:</strong> @ViewBag.LastHarvested<a href="#" data-toggle="tooltip" style="padding-left:20px" title="last time your data was updated for public access."><img class="infobutton" src="~/Images/infobutton_60.jpg" /></a></p>
        @*<p><strong>Last Harvested:</strong>@ViewBag.LastHarvested</p>*@
        @if (ViewBag.NetworkId == "unknown")
        {
            <form method="post">
            <button class="btn btn-primary active" id="RequestPublication" value="val_1" name="buttonRequestPublication">Request First Time Publication</button>  
                <a href="#" data-toggle="tooltip" style="padding-left:20px" title="Request to make your data accessible to the public."><img class="infobutton" src="~/Images/infobutton_60.jpg" /></a>
            </form>
        }
        else
        {
            <h5>
                Uploaded Data is made public automatically every Day.For immediate publication, to turn your data private, or for any data inquiries, please send requests to <a href="mailto:Help@cuahsi.org?Subject=Publication" target="_top">Help@cuahsi.org</a>
            </h5>
        }
            
    </div>
        <div class="panel-heading">
            <h3 class="panel-title">Number of values</h3>
        </div>
        <div class="panel-body">
            <div class="">
                <ul class="list-group">
                    <li class="list-group-item">
                        <span class="badge">@Model.SiteCount</span>Sites
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.VariablesCount</span>Variables
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.SourcesCount</span>Sources
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.MethodsCount</span>Methods
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.DataValuesCount</span>Data Values
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.SeriesCatalog</span>Timeseries
                    </li>
                    <hr>
                    <li class="list-group-item">
                        <span class="badge">@Model.OffsetTypesCount</span>Offset Types
                    </li>

                    <li class="list-group-item">
                        <span class="badge">@Model.LabMethodsCount</span>Lab Methods
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.SamplesCount</span>Samples
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.QualifiersCount</span>Qualifiers
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.QualityControlLevelsCount</span>Quality Control Levels
                    </li>

                    <li class="list-group-item">
                        <span class="badge">@Model.GroupDescriptionsCount</span>Group Description
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.GroupsCount</span>Groups
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.DerivedFromCount</span>Derived From
                    </li>
                    <li class="list-group-item">
                        <span class="badge">@Model.CategoriesCount</span>Categories
                    </li>


                </ul>
            </div>
        </div>
    </div>
